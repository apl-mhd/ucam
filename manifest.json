{
  "name": "UCAM Course Evaluation",
  "version": "1.02",
  "description": "Build an Extension!",
  "manifest_version": 2,
  "content_scripts": [
   {
     "matches": ["http://*.uiu.ac.bd/*"],
     "js": ["content.js"]
   }
 ],

  "background":{
    "scripts" : ["background.js"]
  },

  "permissions": ["tabs"],


  "browser_action":{
    "default_icon": {
        "16": "images/32.png",
        "32": "images/32.png",
        "48": "images/48.png",
        "128": "images/128.png"
      },
 
        "default_popup" : "popup.html",
     "default_title" : "UCAM Browser Action"

    
  },

  "icons": {
      "16": "images/16.png",
      "32": "images/32.png",
      "48": "images/48.png",
      "128": "images/128.png"
    }
  
}